{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "no-alert": "off",
        "no-use-before-define": "off",
        "no-shadow": "off",
        "no-undef": "off",
        "import/prefer-default-export": "off",
        "consistent-return": "off",
        "no-prototype-builtins": "off",
        "eqeqeq": "off",
        "max-len": ["error", {
            "code": 120
        }]
      }
}
